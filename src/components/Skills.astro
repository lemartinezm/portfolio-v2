---

<section id="skills">
  <h2>Skills</h2>
  <ul>
    {skills.map((skill) => <SkillCard {...skill} />)}
  </ul>
</section>

<style>
  * {
    margin: 0;
  }

  section {
    padding: 3rem 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    gap: 1rem;
  }

  h2 {
    font-size: 2rem;
  }

  p {
    font-size: 1.125rem;
  }

  ul {
    padding: 0;
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(8rem, 1fr));
    gap: 1rem;
    box-sizing: border-box;
  }

  @media (min-width: 768px) {
    h2 {
      font-size: 3rem;
    }
  }
</style>
